<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_sale_order_warning_wizard_form" model="ir.ui.view">
            <field name="name">sale.order.warning.wizard.form</field>
            <field name="model">sale.order.warning.wizard</field>
            <field name="arch" type="xml">
                <form string="Warning">
                    <group>
                        <field name="message" readonly="1" nolabel="1"/>
                    </group>
                    <footer>
                        <button string="OK" type="object" name="action_confirm" class="btn-primary"/>
                        <button string="Cancel" special="cancel" class="btn-secondary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>